  "dependencies": {
    "@tailwindcss/vite": "^4.0.15",
    "@unhead/vue": "^2.0.0-rc.13",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "nodemon": "^3.1.9",
    "tailwindcss": "^4.0.15",
    "vite-plugin-ssr": "^0.4.142",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "vite": "^6.2.0"
  }
}

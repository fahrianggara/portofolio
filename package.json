{
  "name": "portofolio-v5",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "prod": "cross-env NODE_ENV=production nodemon server.js",
    "preview": "vite preview",
    "build-client": "vite build --outDir dist/client",
